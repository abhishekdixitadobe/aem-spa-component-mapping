{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "noEmit": false,
        "baseUrl": ".",
        "paths": {
            "@abhishekdixitadobe/aem-spa-component-mapping": [ "./" ],
            "@abhishekdixitadobe/aem-spa-component-mapping/*": [ "./*" ]
        },
        "composite": true,
        "module": "es2015",
        "target": "esnext",
        "importHelpers": true,
        "removeComments": false,
        "declaration": true,
        "declarationMap": true,
        "declarationDir": "../dist",
        "emitDeclarationOnly": true
    }
}